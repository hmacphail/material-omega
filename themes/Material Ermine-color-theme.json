            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Language methods",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": [
                "entity.name.method.js"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
